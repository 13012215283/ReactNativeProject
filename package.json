{
  "name": "MyReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "before-release-and": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res-reactnative/",
    "before-release-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ~/IOSREACT/index.ios.bundle --assets-dest ~/IOSREACT/res/"

  },
  "dependencies": {
    "react": "^16.0.0-beta.5",
    "react-native": "^0.49.3",
    "react-navigation": "git+https://github.com/react-community/react-navigation.git#ed2fc9a09e2f562ae9d3fc7f5df17c593989db0a",
    "axios": "^0.17.0",
    "es6-promise": "^4.1.1",
    "md5": "^2.2.1",
    "qs": "^6.5.1"
  }
}
